    "react-native-gesture-handler": "^1.5.2",
    "react-native-material-menu": "^1.0.0",
    "react-native-modal": "^11.5.3",
    "react-native-modalize": "^1.3.7-rc.10",
    "react-native-music-metadata": "https://github.com/venepe/react-native-music-metadata",
